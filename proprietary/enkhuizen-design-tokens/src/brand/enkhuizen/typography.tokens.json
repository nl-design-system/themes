{
  "enkhuizen": {
    "typography": {
      "sans-serif": {
        "font-family": {
          "$value": "Arial,-apple-system,BlinkMacSystemFont,Segoe UI,Source Sans Pro,Helvetica Neue,sans-serif,Apple Color Emoji,Segoe UI Emoji,Segoe UI Symbol"
        }
      },
      "scale": {
        "md": {
          "font-size": {
            "$value": "1rem"
          },
          "line-height": {
            "$value": "1.75"
          }
        },
        "sm": {
          "font-size": {
            "$value": "14px"
          },
          "line-height": {
            "$value": "1.75"
          }
        }
      },
      "weight-scale": {
        "bold": {
          "font-weight": {
            "$value": "700"
          }
        },
        "semi-bold": {
          "font-weight": {
            "$value": "500"
          }
        },
        "normal": {
          "font-weight": {
            "$value": "400"
          }
        }
      }
    }
  }
}
