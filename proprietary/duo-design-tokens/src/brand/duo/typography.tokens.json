{
  "duo": {
    "typography": {
      "sans-serif": {
        "font-family": {
          "$value": "RijksoverheidSans, Helvetica, Arial, sans-serif"
        }
      },
      "scale": {
        "md": {
          "font-size": {
            "$value": "1.8rem"
          },
          "line-height": {
            "$value": "2.6rem"
          }
        },
        "sm": {
          "font-size": {
            "$value": "1.6rem"
          },
          "line-height": {
            "$value": "2.2rem"
          }
        }
      },
      "weight-scale": {
        "bold": {
          "font-weight": {
            "$value": "700"
          }
        },
        "semi-bold": {
          "font-weight": {
            "$value": "500"
          }
        },
        "normal": {
          "font-weight": {
            "$value": "400"
          }
        }
      }
    }
  }
}
