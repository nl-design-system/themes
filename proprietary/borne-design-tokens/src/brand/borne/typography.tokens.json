{
  "borne": {
    "typography": {
      "sans-serif": {
        "font-family": {
          "$value": "\"Droid Sans W01\",-apple-system,\"BlinkMacSystemFont\",\"Segoe UI\",\"Roboto\",\"Helvetica Neue\",\"Arial\",\"Noto Sans\",sans-serif,\"Apple Color Emoji\",\"Segoe UI Emoji\",\"Segoe UI Symbol\",\"Noto Color Emoji\""
        }
      },
      "scale": {
        "md": {
          "font-size": {
            "$value": "16px"
          },
          "line-height": {
            "$value": "22px"
          }
        },
        "sm": {
          "font-size": {
            "$value": "14px"
          },
          "line-height": {
            "$value": "18px"
          }
        }
      },
      "weight-scale": {
        "bold": {
          "font-weight": {
            "$value": "700"
          }
        },
        "semi-bold": {
          "font-weight": {
            "$value": "500"
          }
        },
        "normal": {
          "font-weight": {
            "$value": "400"
          }
        }
      }
    }
  }
}
