{
  "tilburg": {
    "typography": {
      "sans-serif": {
        "font-family": {
          "$value": "Arial, Helvetica, sans-serif"
        }
      },
      "special": {
        "font-family": {
          "$value": "\"Trade Gothic W01 Cn 18\", AvenirNextCondensed-Medium, \"Univers Condensed\", Univers, Arial, Helvetica, sans-serif"
        }
      },
      "heading": {
        "font-family": {
          "$value": "\"TradeGothicW01-BoldCn20 675334\", AvenirNextCondensed-DemiBold, \"Univers Condensed\", Univers, Arial, Helvetica, sans-serif"
        }
      },
      "scale": {
        "md": {
          "font-size": {
            "$value": "16px"
          },
          "line-height": {
            "$value": "22px"
          }
        },
        "sm": {
          "font-size": {
            "$value": "14px"
          },
          "line-height": {
            "$value": "18px"
          }
        }
      },
      "weight-scale": {
        "bold": {
          "font-weight": {
            "$value": "700"
          }
        },
        "semi-bold": {
          "font-weight": {
            "$value": "500"
          }
        },
        "normal": {
          "font-weight": {
            "$value": "400"
          }
        }
      }
    }
