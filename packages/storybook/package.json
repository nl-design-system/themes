{
  "version": "1.0.0-alpha.0",
  "author": "Community for NL Design System",
  "description": "Storybook for NL Design System themes",
  "keywords": [
    "nl-design-system"
  ],
  "license": "EUPL-1.2",
  "name": "@nl-design-system-unstable/storybook",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.js",
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/themes.git"
  },
  "scripts": {
    "clean": "rimraf dist/ src/generated/",
    "prebuild": "npm run clean",
    "build": "npm-run-all --sequential build:generated-stories build:storybook",
    "build:generated-stories": "ts-node --project ./tsconfig.commonjs.json src/generate-component-pages",
    "build:storybook": "build-storybook --config-dir config/ --output-dir dist/",
    "storybook": "npm-run-all --sequential clean build:generated-stories dev:storybook",
    "dev:storybook": "start-storybook --config-dir config/ --no-manager-cache --port 6006"
  },
